{
  "name": "orangee",
  "version": "1.0.0",
  "description": "A discord.js tool for making bots great, and blazing fast.",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "yolocat",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.2"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": ["Discord", "Discord.JS", "bot", "fast", "yolocat", "orange", "orangee"]
}
